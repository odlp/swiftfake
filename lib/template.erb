<%= fake_class_signature %> {

<% functions.each do |f| -%>
    <%= f.signature %> {
        <%= f.call_count %> += 1
        <% if f.has_args? %><%= f.args_store_append %><% end %>
        <% if f.returns? %><%= f.return_value_declaration %><% end %>
    }

<% end %>

    // MARK: - Fake Helpers

<% functions.each do |f| -%>
    <%= f.call_count_declaration %>
<% end %>

<% functions_with_args.each do |f| -%>
    <%= f.args_store_declaration %>
<% end %>
}
